{
  "name": "autonomous-browser-agent",
  "version": "1.0.0",
  "description": "AI-powered autonomous browser automation system",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm start",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "npm run dev"
  },
  "keywords": [
    "browser-automation",
    "ai",
    "puppeteer",
    "autonomous-agent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
